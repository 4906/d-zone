{
	"compilerOptions": {
		"target": "es2019",
		"lib": ["es2019", "dom"],
		"module": "commonjs",

		"sourceMap": true,

		"strict": true,

		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,

		"moduleResolution": "node",

		"esModuleInterop": true,

		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,

		"resolveJsonModule": true
	}
}
